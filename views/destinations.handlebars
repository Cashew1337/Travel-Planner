{{#each destinations as |destination|}}
<div class="grid-container">
    <div class="box">
        <div class="card">
            <h2 class="destination-page-name">{{name}}</h2>
            <a href="/destination/{{id}}">
        </div>
    </div>
    {{log destination}}

    {{#each trips as |trip|}}
    <div class="box">
        <div class="destination-card">
           {{!--  <p class="destination-display">{{trip.name}}</p> --}}
            <a href="/trips/{{trip.id}}" class="link-to-trip">
             <img src="{{destination.imageUrl}}" style="max-width: 200px; max-height: 200px;">
            </a>

        </div>
        {{/each}}
        </div>
        {{/each}}
        </div>

