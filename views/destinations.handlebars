{{#each destinations as |destination|}}
<div class="grid-container">
    <div class="box">
        <div class="card">
            <h2>{{name}}</h2>
            <img src="https://placebear.com/640/360" />
        </div>
    </div>

    <div class="box-text">
        <h2>Upcoming Trips</h2>
    </div>

    {{#each trips as |trip|}}
    <div class="box">
        <div class="card">
            <p>{{trip.name}}</p>
            <img src="https://placebear.com/640/360" />
        </div>
        {{/each}}
        {{/each}}